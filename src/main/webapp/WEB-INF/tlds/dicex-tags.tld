<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
    <tlib-version>1.0.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>dicex</short-name>
    <uri>/dicex-tags</uri>
    <tag>
        <name>reqTypeTag</name>
        <tag-class>com.jc.csmp.plan.tag.ReqTypeDicTag</tag-class>
        <attribute><name>name</name></attribute>
        <attribute><name>type</name><description>eg:edu</description></attribute>
        <attribute><name>id</name></attribute>
        <attribute><name>parentId</name></attribute>
        <attribute><name>headName</name></attribute>
        <attribute><name>headValue</name></attribute>
        <attribute><name>filter</name></attribute>
        <attribute><name>size</name></attribute>
        <attribute><name>cssClass</name></attribute>
        <attribute><name>cssStyle</name></attribute>
        <attribute><name>onclick</name></attribute>
        <attribute><name>onfocus</name></attribute>
        <attribute><name>onblur</name></attribute>
        <attribute><name>onchange</name></attribute>
        <attribute><name>defaultValue</name></attribute>
    </tag>
    <tag>
        <name>typeTag</name>
        <tag-class>com.jc.csmp.doc.tag.MechTypeDicTag</tag-class>
        <attribute><name>name</name></attribute>
        <attribute><name>type</name><description>eg:edu</description></attribute>
        <attribute><name>id</name></attribute>
        <attribute><name>parentId</name></attribute>
        <attribute><name>headName</name></attribute>
        <attribute><name>headValue</name></attribute>
        <attribute><name>filter</name></attribute>
        <attribute><name>size</name></attribute>
        <attribute><name>cssClass</name></attribute>
        <attribute><name>cssStyle</name></attribute>
        <attribute><name>onclick</name></attribute>
        <attribute><name>onfocus</name></attribute>
        <attribute><name>onblur</name></attribute>
        <attribute><name>onchange</name></attribute>
        <attribute><name>defaultValue</name></attribute>
    </tag>
</taglib>
